{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "PractikaSecretsRead",
            "Effect": "Allow",
            "Action": [
                "secretsmanager:GetSecretValue",
                "secretsmanager:DescribeSecret"
            ],
            "Resource": [
                "arn:aws:secretsmanager:*:*:secret:practika/production/*",
                "arn:aws:secretsmanager:*:*:secret:practika/staging/*"
            ]
        }
    ]
}
